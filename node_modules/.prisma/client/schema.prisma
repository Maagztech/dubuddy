generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model ModelRecord {
  id        String   @id @default(uuid())
  model     String
  data      String
  ownerId   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
